<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>24.1.10.1</Version>
    <Description>Read input and write output to prenamed files in every app on sunamo's platform</Description>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoFileIO" Version="24.1.7.1" />
    <PackageReference Include="SunamoPlatformUwpInterop" Version="24.1.5.1" />
    <PackageReference Include="SunamoStringJoin" Version="24.1.5.1" />
    <PackageReference Include="SunamoValues" Version="24.1.9.2" />
    <PackageReference Include="SunamoWinStd" Version="24.1.5.1" />
    <PackageReference Include="TextCopy" Version="6.2.1" />
  </ItemGroup>
</Project>